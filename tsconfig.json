{
  "compilerOptions": {
    "types": ["@testing-library/jest-dom"],
    "outDir": "./dist/",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node"
  }
}
